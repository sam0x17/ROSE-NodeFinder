include $(top_srcdir)/config/Makefile.for.ROSE.includes.and.libs

## Place generated object files (.o) into the same directory as their source
## files, in order to avoid naming collisions when non-recursive make is used.
AUTOMAKE_OPTIONS = subdir-objects

SUBDIRS=\
	no_vfork \
	no_variadic_functions \
	no_goto \
	floating_point_exact_comparison \
	no_rand \
	allocate_and_free_in_the_same_module \
	asynchronous_signal_handler \
	discard_assignment \
	explicit_test_for_non_boolean_value \
	data_member_access \
	unary_minus \
	pointer_comparison \
	do_not_delete_this \
	size_of_pointer \
	float_for_loop_counter \
	ternary_operator \
	forbidden_functions \
	magic_number \
	dangerous_overload \
	comma_operator \
	byte_by_byte_structure_comparison \
	boolean_is_has \
	dead_function \
	default_argument \
	function_prototype \
	function_with_multiple_returns \
	global_variables \
	keyword_macro \
	non_global_cpp_directive \
	non_static_array_size \
	variable_name_similarity
